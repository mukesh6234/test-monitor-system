{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "rules": {
      "no-throw-literal": 0,
      "no-undef": 0,
      "react/prop-types": 0,
      "react/display-name": 0,
      "react-hooks/exhaustive-deps": 0
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "settings": {
        "react": {
        "version": "detect"
        }
      }
    }